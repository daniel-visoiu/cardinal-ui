{
  "workDir": ".",
  "dependencies": [
    {
      "name": "cardinal-core",
      "src": "https://github.com/daniel-visoiu/cardinal-core.git",
      "actions": [
        {
          "type": "smartClone",
          "target": "./",
          "collectLog": false
        },
        {
          "type": "execute",
          "cmd": "cd ./cardinal-core && npm install"
        },
        {
          "type": "execute",
          "cmd": "cd ./cardinal-core && npm run build"
        }
      ]
    }
  ]
}
